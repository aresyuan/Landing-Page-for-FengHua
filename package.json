    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.0.7",
    "husky": "^0.14.3",
    "image-webpack-loader": "^4.2.0",
    "install": "^0.10.4",
    "lint-staged": "^7.0.5",
    "node-sass": "^4.9.0",
    "prettier": "^1.12.1",
    "prettier-package-json": "^1.5.1",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.20.3",
    "url-loader": "^1.0.1",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.12",
    "webpack-dev-server": "^3.1.1"
